{
  "version": 2,
  "rewrites": [
    {
      "source": "/framerusercontent.com/(.*)",
      "destination": "/framerusercontent.com/$1"
    },
    {
      "source": "/api.framer.com/(.*)",
      "destination": "/api.framer.com/$1"
    },
    {
      "source": "/app.framerstatic.com/(.*)",
      "destination": "/app.framerstatic.com/$1"
    },
    {
      "source": "/framer.com/(.*)",
      "destination": "/framer.com/$1"
    },
    {
      "source": "/events.framer.com/(.*)",
      "destination": "/events.framer.com/$1"
    },
    {
      "source": "/_DataURI/(.*)",
      "destination": "/_DataURI/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.{js,mjs}",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}