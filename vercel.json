{
  "version": 2,
  "rewrites": [
    {
      "source": "/(app\\.framerstatic\\.com|framerusercontent\\.com|framer\\.com|events\\.framer\\.com|api\\.framer\\.com|_DataURI)/(.*)",
      "destination": "/$1/$2"
    },
    {
      "source": "/(.*)\\.(js|mjs|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|mp4|webm|map)",
      "destination": "/$1.$2"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}